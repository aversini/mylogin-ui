{
	"name": "@mylogin/client",
	"version": "1.5.3",
	"license": "MIT",
	"author": "Arno Versini",
	"type": "module",
	"scripts": {
		"bundlesize": "bundlemon",
		"clean": "rimraf dist",
		"dev": "vite --host",
		"build:check": "tsc",
		"build:js": "vite build",
		"build": "npm-run-all --serial clean build:check build:js",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix --color",
		"start": "static-server dist --port 5173",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage",
		"test:coverage:ui": "vitest --coverage --ui",
		"test:watch": "vitest"
	},
	"dependencies": {
		"@versini/ui-components": "3.2.2",
		"autoprefixer": "10.4.16",
		"clsx": "2.0.0",
		"json5": "2.2.3",
		"postcss": "8.4.31",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"tailwindcss": "3.3.5",
		"uuid": "9.0.1"
	}
}
